{
  "name": "gatsby-minimal-simple-blog",
  "description": "A minimally simple starter package to build a static blogging site using Gatsbyjs",
  "version": "1.0.0",
  "author": "An Tran",
  "bugs": {
    "url": "https://github.com/antranapp/gatsby-minimal-simple-blog"
  },
  "dependencies": {
    "gatsby": "3.7.2",
    "gatsby-plugin-feed": "3.7.1",
    "gatsby-plugin-i18n": "1.0.1",
    "gatsby-plugin-image": "1.7.1",
    "gatsby-plugin-manifest": "3.7.1",
    "gatsby-plugin-plausible": "0.0.7",
    "gatsby-plugin-react-helmet": "4.7.1",
    "gatsby-plugin-sharp": "3.7.1",
    "gatsby-plugin-sitemap": "3.3.0",
    "gatsby-remark-autolink-headers": "4.4.1",
    "gatsby-remark-copy-linked-files": "4.4.1",
    "gatsby-remark-images": "5.4.1",
    "gatsby-remark-prismjs": "5.4.1",
    "gatsby-remark-responsive-iframe": "4.4.1",
    "gatsby-remark-smartypants": "4.4.1",
    "gatsby-source-filesystem": "3.7.1",
    "gatsby-transformer-remark": "4.4.1",
    "gatsby-transformer-sharp": "3.7.1",
    "moment-timezone": "0.5.33",
    "prismjs": "1.24.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "reading-time": "1.3.0"
  },
  "devDependencies": {
    "cypress": "7.5.0",
    "gh-pages": "3.2.3",
    "jest": "27.0.5",
    "prettier": "2.3.2",
    "start-server-and-test": "1.12.5"
  },
  "homepage": "https://github.com/antranapp/gatsby-minimal-simple-blog",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antranapp/gatsby-minimal-simple-blog.git"
  },
  "scripts": {
    "bootstrap": "npm install",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:unit": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "test:e2e": "npm run build && start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "npm run build && start-server-and-test develop http://localhost:8000 cy:run"
  }
}
